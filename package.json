{
  "name": "gram-frontend",
  "version": "0.4.0",
  "description": "Frontend for the Gram Telegram web client",
  "scripts": {
    "build": "webpack --config webpack.dev.js",
    "serve": "webpack-dev-server --config webpack.dev.js --open",
    "watch": "webpack --watch --config webpack.dev.js",
    "prod": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaguelyserious/gram-fronetnd.git"
  },
  "keywords": [
    "Telegram",
    "Gram",
    "Chat"
  ],
  "author": "Peter Wielander",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/VaguelySerious/gram-node-frontend/issues"
  },
  "homepage": "https://github.com/VaguelySerious/gram-node-frontend",
  "dependencies": {
    "axios": "^0.18.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "dotenv": "^6.2.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "svelte": "^2.16.0",
    "svelte-loader": "^2.12.0",
    "terser-webpack-plugin": "^1.2.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "eslintConfig": {
    "globals": {
      "window": true,
      "document": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    }
  }
}
